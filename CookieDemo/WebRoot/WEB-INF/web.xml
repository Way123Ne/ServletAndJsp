  </servlet>
  
  <servlet-mapping>
  	<servlet-name>cookdemo2</servlet-name>
	<url-pattern>/cookdemo2</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>cookdemo3</servlet-name>
  	<servlet-class>com.wayne.cookie.CookieDemo3</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>cookdemo3</servlet-name>
	<url-pattern>/cookdemo3</url-pattern>
  </servlet-mapping>
</web-app>
